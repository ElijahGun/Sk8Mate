<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('./partials/head.ejs')  %> 
    <script src='https://api.mapbox.com/mapbox-gl-js/v2.9.1/mapbox-gl.js'></script>
<link href='https://api.mapbox.com/mapbox-gl-js/v2.9.1/mapbox-gl.css' rel='stylesheet' />

    <title>>Sk8Mate<</title>
</head>
<body>

    <%- include('./partials/navbar.ejs')  %> 

    <div id="map" style="width:100%; height:500px"></div>

    <h1>All SkateParks</h1>

    <%= success %> 

<h2>SkateParks</h2>
<% for (park of skateparks) {%>
    
    <div class="card" style="width: 18rem;">
      <% if(park.images.length) {%> 
        <img src="<%=park.images[0].url%>" class="card-img-top" alt="...">
      <% } else {%>
        <img src="data:image/jpeg;base64," alt="" srcset=""> 
        <% } %> 
        <div class="card-body">
          <h5 class="card-title"><%= park.name  %> </h5>
          <p class="card-text"><%= park.location  %> </p>
          <a href="/skateparks/<%= park._id %> " class="btn btn-primary">Details</a>
        </div>
      </div>
<% } %>  

<script> 
  const mapToken = '<%-process.env.MAPBOX_TOKEN%>';
  const skateparks = {features: <%-JSON.stringify(skateparks)%>};
</script>

<%- include('./partials/footer.ejs')  %> 
<script src="/javascripts/clusterMap.js"></script>
</body>
</html>